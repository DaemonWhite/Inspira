<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="inspira">
	<schema id="@URI@" path="@URI_PATH@/">
	  <key name="auto-save-tags" type="b">
      <default>false</default>
      <summary>Save no know tags for autocomplets</summary>
      <description></description>
    </key>
	  <key name="carousel-images-is-vertical" type="b">
      <default>false</default>
      <summary>If the image scrolling is horizontal or vertical. False=horizontal</summary>
      <description></description>
    </key>
	  <key name="global-nsfw" type="b">
      <default>false</default>
      <summary>Display posibility to enabled nsfw</summary>
      <description></description>
    </key>
	  <key name="switch-last-add-image" type="b">
      <default>true</default>
      <summary>Focus on last image download</summary>
      <description></description>
    </key>
	  <key name="timeout-download" type="i">
      <default>-1</default>
      <summary>Delay in seconds. Use -1 for API default timeout</summary>
      <description></description>
	    <range min="-1" max="100"/>
    </key>
	</schema>
</schemalist>
